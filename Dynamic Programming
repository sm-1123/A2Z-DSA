P9:Triangle

SPACE OPTIMIZATION[TC:O(N*N),SC:O(N)]

def minimumPathSum(triangle, n):

    front = [0] * n

    curr = [0] * n

    for j in range(n):

        front[j] = triangle[n-1][j]

    for i in range(n-2, -1, -1):

        for j in range(i+1):

            down = triangle[i][j] + front[j]

            diag = triangle[i][j] + front[j+1]

            curr[j] = min(down, diag)

        front = curr

    return front[0]        

    # Write your code here.

P10:Minimum/Maximum Falling Path Sum

SPACE OPTIMIZATON[TC:O(N*M),SC:O(M)]

def getMaxPathSum(matrix):

    n = len(matrix)

    m = len(matrix[0])

    prev = [0] * m

    curr = [0] * m

    for j in range(m):

        prev[j] = matrix[0][j]

    for i in range(1, n):

        for j in range(m):

            up = matrix[i][j] + prev[j]

            leftDiag = matrix[i][j]

            if j - 1 >= 0:

                leftDiag += prev[j - 1]

            else:

                leftDiag = -int(1e9)

            rightDiag = matrix[i][j]

            if j + 1 < m:

                rightDiag += prev[j + 1]

            else:

                rightDiag = -int(1e9)

            curr[j] = max(up, max(leftDiag, rightDiag))

        prev = curr[:]    

    maxi = -sys.maxsize

    for j in range(m):

        maxi = max(maxi, prev[j])

    return maxi

P11:Ninja and his friends

SPACE OPTIMIZATION[TC:O(N*M*M),SC:O(M*M)]

def maximumChocolates(r: int, c: int, grid: List[List[int]]) -> int:

    front = [[0] * c for _ in range(c)]

    curr = [[0] * c for _ in range(c)]

    for j1 in range(c):

        for j2 in range(c):

            if j1 == j2:

                front[j1][j2] = grid[r-1][j1]

            else:

                front[j1][j2] = grid[r-1][j1] + grid[r-1][j2]

    for i in range(r-2, -1, -1):

        for j1 in range(c):

            for j2 in range(c):

                maxi = -sys.maxsize

                for di in range(-1,2):

                    for dj in range(-1,2):

                        ans = 0

                        if j1 == j2:

                            ans = grid[i][j2]

                        else:

                            ans = grid[i][j1] + grid[i][j2]

                        if ((j1 + di < 0 or j1 + di >= c) or (j2 + dj < 0 or j2 + dj >= c)):

                            ans += int(-1e9)

                        else:

                            ans += front[j1 + di][j2 + dj]

                        maxi = max(ans, maxi)

                curr[j1][j2] = maxi

        front = [row[:] for row in curr]

    return front[0][c-1]                    

    # write your code here
